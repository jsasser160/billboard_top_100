<h1>Pages#edit</h1>
<%= form_for @page do |f| %>
  <%= f.label :title %>
  <% f.text_field :title %>

  <%= f.label :artist %>
  <% f.text_field :artist%>

  <%= f.label :song_name%>
  <% f.text_field :song_name%>

  <%= f.label :album %>
  <% f.text_field :album %>

  <%= f.submit %>
<% end %>
  
<p>Find me in app/views/pages/edit.html.erb</p>
