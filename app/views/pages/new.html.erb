<h1>New Page</h1>
<%= form_for @pages do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :artist_name%>
  <%= f.text_field :artist_name %>

  <%= f.label :song_name %>
  <%= f.text_field :song_name%>

  <%= f.label :album %>
  <%= f.text_field :album%>

  <%= f.submit %>
<% end %>
<p>Find me in app/views/pages/new.html.erb</p>
